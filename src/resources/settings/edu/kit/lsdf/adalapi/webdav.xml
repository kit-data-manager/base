<?xml version="1.0" encoding="ISO-8859-1" ?>

<protocol>
  <http>
    <identifier>http+dav</identifier>
    <protocolClass>edu.kit.lsdf.adalapi.protocols.WebDav</protocolClass>
    <authClass>edu.kit.lsdf.adalapi.authentication.UserPasswordAuthentication</authClass>
